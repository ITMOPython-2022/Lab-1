import matplotlib.pyplot as plt

# Create a new figure and axes
fig, ax = plt.subplots()

# Draw two black circles
circle1 = plt.Circle((0.3, 0.5), 0.2, color='black')  # First circle
circle2 = plt.Circle((0.7, 0.5), 0.2, color='black')  # Second circle

# Add the circles to the axes
ax.add_artist(circle1)
ax.add_artist(circle2)

# Set the aspect ratio and limits for proper display
ax.set_aspect('equal')
ax.set_xlim(0, 1)
ax.set_ylim(0, 1)

# Remove axes for a clean look
ax.axis('off')

# Display the plot
plt.show()
